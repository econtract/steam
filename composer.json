{
    "name": "econtract/steam",
    "description": "E-Contract BVBA Steam API package",
    "version": "2.6.1",
    "keywords": ["E-Contract", "Aanbieders", "Steam"],
    "homepage": "https://aanbieders.be",
    "license": "proprietary",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "~5.1",
        "ixudra/curl": "~6.0",
        "vlucas/phpdotenv": "~2.0"
    },

    "autoload": {
        "psr-4": {
            "Econtract\\Steam\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Econtract\\Steam\\SteamServiceProvider"
            ],
            "aliases": {
                "Steam": "Econtract\\Steam\\Facades\\Steam"
            }
        }
    },

    "minimum-stability": "stable"
}
